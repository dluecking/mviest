localrule count_virome_contigs:
    input: results/mv1/contigs/virome.contigs.filtered.fasta
    output: results/mv1/enough_filtered_virome_contigs.txt
    jobid: 10
    reason: Missing output files: results/mv1/enough_filtered_virome_contigs.txt; Input files updated by another job: results/mv1/contigs/virome.contigs.filtered.fasta
    wildcards: sample=mv1
    resources: tmpdir=/tmp


        n=$(grep '>' results/mv1/contigs/virome.contigs.filtered.fasta  | wc -l)
        if [[ $n -ge 1 ]]
        then
            touch results/mv1/enough_filtered_virome_contigs.txt
        else
            touch results/mv1/NOT_enough_filtered_virome_contigs.txt
        fi
        
