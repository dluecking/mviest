localrule count_virome_contigs:
    input: results/mv3/contigs/virome.contigs.filtered.fasta
    output: results/mv3/enough_filtered_virome_contigs.txt
    jobid: 122
    reason: Missing output files: results/mv3/enough_filtered_virome_contigs.txt; Input files updated by another job: results/mv3/contigs/virome.contigs.filtered.fasta
    wildcards: sample=mv3
    resources: tmpdir=/tmp


        n=$(grep '>' results/mv3/contigs/virome.contigs.filtered.fasta  | wc -l)
        if [[ $n -ge 1 ]]
        then
            touch results/mv3/enough_filtered_virome_contigs.txt
        else
            touch results/mv3/NOT_enough_filtered_virome_contigs.txt
        fi
        

